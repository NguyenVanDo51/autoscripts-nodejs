{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 3,
  "name": "Hudo NodeJS Airdrop Automation",
  "description": "Hudo NodeJS Airdrop Automation",
  "version": "1.0.1",
  "permissions": ["tabs", "alarms"],
  "host_permissions": ["*://*/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>", "*://*/*"],
      "js": ["content.js"],
      "all_frames": true
    }
  ]
}
